{
  "name": "coc-zi",
  "version": "1.0.0",
  "description": "words for coc.nvim",
  "keywords": [
    "coc.nvim",
    "words",
    "english",
    "zi"
  ],
  "main": "./out/index.js",
  "repository": "https://github.com/iamcco/coc-zi",
  "author": "iamcco <ooiss@qq.com>",
  "license": "MIT",
  "engines": {
    "coc": "^0.0.74"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "title": "coc-zi configuration",
      "properties": {
        "zi.enable": {
          "type": "boolean",
          "default": true,
          "description": "enable coc-zi"
        },
        "zi.trace.server": {
          "type": "string",
          "default": "off",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "description": "Trace level of log"
        }
      }
    }
  },
  "scripts": {
    "build": "rm -rf ./out && webpack",
    "watch": "webpack -w",
    "lint": "eslint ./src/**/*.ts",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/got": "^9.6.7",
    "@types/tunnel": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "coc.nvim": "^0.0.74",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "got": "^9.6.0",
    "prettier": "^1.18.2",
    "ts-loader": "^6.2.1",
    "tunnel": "^0.0.6",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "@types/node": "^12.11.7",
    "vscode-languageserver-protocol": "^3.14.1"
  }
}
